{
  "pluginAlias": "homebridge-unifi-access",
  "pluginType": "platform",
  "singular": true,
  "customUi": true,
  "schema": {
    "properties": {
      "doorName": {
        "title": "Name of the door",
        "type": "string",
        "required": false
      },
      "doorId": {
        "title": "Id of the door",
        "type": "string",
        "required": false
      },
      "apiToken": {
        "title": "Unifi API-Token",
        "type": "string",
        "required": true
      },
      "consoleHost": {
        "title": "Unifi Console Host",
        "type": "string",
        "required": true
      },
      "consolePort": {
        "title": "Unifi Console Port",
        "type": "string",
        "default": "12445",
        "required": true
      }
    }
  }
}
